[include]
.*/index.js

[ignore]
.*/node_modules/fbjs/*
.*/node_modules/react-tap-event-plugin/*

[libs]
./interfaces/global.js

[options]
module.name_mapper='.*\(.css\)' -> 'CSSModule'
module.name_mapper='.*\.\(svg\|png\|jpg\|gif\)$' -> 'Images'
module.system=haste
strip_root=true
esproposal.class_static_fields=enable
